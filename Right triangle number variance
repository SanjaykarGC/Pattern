Sample Input 1
7
Sample Output 1
1
21
321
4321
54321
654321
7654321

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,i,j;
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        for(j=i;j>=1;j--){
            printf("%d",j);
        }
        printf("\n");
    }
}
